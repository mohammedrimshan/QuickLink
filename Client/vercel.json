{
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/assets/$1" },

    {
      "source": "/s/:shortUrl",
      "destination": "https://api.quicklink.rimshan.in/api/pvt/:shortUrl"
    },

    { "source": "/", "destination": "/" },
    { "source": "/login", "destination": "/" },
    { "source": "/register", "destination": "/" },

    { "source": "/profile", "destination": "/" },
    { "source": "/my-links", "destination": "/" },
    { "source": "/analytics/:urlId", "destination": "/" },
    { "source": "/settings", "destination": "/" },
    { "source": "/help", "destination": "/" },

    { "source": "/(.*)", "destination": "/" }
  ]
}
